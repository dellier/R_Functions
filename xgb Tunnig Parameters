xgbGrid <- expand.grid(nrounds = c(100, 200, 300, 500, 700, 1000),
                       max_depth = c(3, 5, 7, 9, 12),
                       eta = c(.1, .3),
                       gamma = 0,
                       colsample_bytree = .7,
                       min_child_weight = 1,
                       subsample = c(.8))
                       
               
